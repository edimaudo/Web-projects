{"head":"<title>Meteor Chat Application.</title>","body":"","js":"\nTemplate.body.addContent((function() {\n  var view = this;\n  return [ HTML.Raw(\"<h1>Chatapp</h1>\\n  \"), Spacebars.include(view.lookupTemplate(\"welcome\")), \"\\n  \", Spacebars.include(view.lookupTemplate(\"input\")), \"\\n  \", Spacebars.include(view.lookupTemplate(\"messages\")) ];\n}));\nMeteor.startup(Template.body.renderToDocument);\n\nTemplate.__checkName(\"welcome\");\nTemplate[\"welcome\"] = new Template(\"Template.welcome\", (function() {\n  var view = this;\n  return HTML.Raw(\"<p>\\n    Let's chat.\\n  </p>\");\n}));\n\nTemplate.__checkName(\"messages\");\nTemplate[\"messages\"] = new Template(\"Template.messages\", (function() {\n  var view = this;\n  return Blaze.Each(function() {\n    return Spacebars.call(view.lookup(\"messages\"));\n  }, function() {\n    return [ \"\\n    \", HTML.STRONG(Blaze.View(\"lookup:name\", function() {\n      return Spacebars.mustache(view.lookup(\"name\"));\n    }), \":\"), \" \", Blaze.View(\"lookup:message\", function() {\n      return Spacebars.mustache(view.lookup(\"message\"));\n    }), HTML.BR(), \"\\n  \" ];\n  });\n}));\n\nTemplate.__checkName(\"input\");\nTemplate[\"input\"] = new Template(\"Template.input\", (function() {\n  var view = this;\n  return HTML.Raw('<p>Message: <input type=\"text\" id=\"message\"></p>');\n}));\n","bodyAttrs":{}}